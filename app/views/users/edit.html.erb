<div class="container">
  <div id="user">
      <h1 class="page-name">EDIT</h1>
      <div class="text-form">
          <%= form_with model:@user, local: true do |f| %>
          <div class="form-group">
             <%= f.attachment_field :profile_image %>
          </div>
          <div class="form-group">
             <%= f.text_area :name %>
          </div>
          <div class="form-group">
             <%= f.email_field :email %>
          </div>
          <div class="form-group">
             <%= f.submit class:"btn" %>
          </div>
          <% end %>
      </div>
  </div>
</div>